{
    "plugins": ["jsdoc-http-plugin"],
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs/)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "resource": "true",
        "destination": "./doc", 
        "template": "node_modules/docdash",
        "tutorials": "./tutorials",
        "readme": "./README.md"
    }
}

